        "test": "nx test",
        "test-package": "nx run ngx-cva-test-suite:test",
        "lint-package": "nx run ngx-cva-test-suite:lint",
        "format": "nx format:write",
        "format:write": "nx format:write",
        "format:check": "nx format:check"
